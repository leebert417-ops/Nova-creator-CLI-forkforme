        technical_specs:
          processor: "单核生物处理器"
          storage: "256KB ROM (只读存储)"
          interface: "8条纳米神经接驳探针"
          power: "自供能（葡萄糖→生物能转换）"
          power_consumption: "0.001W (维持) / 0.01W (运行)"
          lifespan: "5-10年（理论）"

        core_modules:
